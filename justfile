# 开发服务器（同时启动主服务器和Socket服务器）

dev:
  npm run dev

# 仅启动主服务器（开发环境）

serve:
  npm run serve

# 仅启动Socket服务器（开发环境）

socket:
  npm run socket

# 格式化代码

prettier:
  npm run prettier

# 更新Prettier配置

update-prettier:
  npm run update:prettier

# 生产环境启动主服务器

start:
  npm run start

# 生产环境启动Socket服务器

start-socket:
  npm run start:socket